<archetype-descriptor name="factory-ui-archetype-all" xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="groupId">
      <defaultValue>org.qualipso</defaultValue>
    </requiredProperty>
    <requiredProperty key="artifactId">
      <defaultValue>factory-ui-service-myservice</defaultValue>
    </requiredProperty>
    <requiredProperty key="version">
      <defaultValue>1.0-QAD-SNAPSHOT</defaultValue>
    </requiredProperty>
    <requiredProperty key="serviceGroupId">
      <defaultValue>org.qualipso</defaultValue>
    </requiredProperty>
    <requiredProperty key="serviceArtifactId">
      <defaultValue>factory-service-myservice</defaultValue>
    </requiredProperty>
    <requiredProperty key="serviceVersion">
      <defaultValue>1.0-SNAPSHOT</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet encoding="UTF-8">
      <directory>src/main/webapp</directory>
      <includes>
        <include>**/*.MF</include>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
